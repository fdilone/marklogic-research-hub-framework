{
  "template": {
    "context": "/envelope[instance/YourEntityName]",
    "collections": ["YourEntityName"],

    "vars": [{

      "name": "PRHPrefix",
      "val": "'PRH:'"

    },{

      "name": "yourentitynameIri",
      "val": "sem:iri(xdmp:node-uri(.))"

    }],

    "templates": [{

      "context": "instance/YourEntityName",

      "triples": [{
        "subject": {
          "val": "$yourentitynameIri"
        },
        "predicate": {
          "val": "sem:iri($PRHPrefix || 'entityType')"
        },
        "object": {
          "val": "lower-case(name(.))"
        }
      },{
        "subject": {
          "val": "$yourentitynameIri"
        },
        "predicate": {
          "val": "sem:iri($PRHPrefix || 'preferredName')"
        },
        "object": {
          "val": "string(name)"
        }
      }]

    }]
  }
}
